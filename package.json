    "test-functions": "tsx scripts/testConflictFunctionsOnly.ts",
    "test-updated": "tsx scripts/testUpdatedFunctions.ts",
    "build:vercel": "cross-env NODE_ENV=production next build",
    "preflight": "npm run type-check && npm run lint && npm run test && npm run build:vercel",
    "analyze": "ANALYZE=true next build",
    "performance": "next build && next start",
    "clean": "rm -rf .next out dist coverage",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
