{
  "name": "home-management-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "generate-icons": "node scripts/generateIcons.js",
    "generate-vapid-keys": "node scripts/generateVapidKeys.js",
    "send-reminders": "tsx scripts/sendReminders.ts",
    "analyze": "ANALYZE=true next build",
    "performance": "next build && next start",
    "clean": "rm -rf .next out dist coverage",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "@clerk/nextjs": "^6.25.5",
    "@heroicons/react": "^2.2.0",
    "@sentry/nextjs": "^10.3.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.52.1",
    "@tanstack/react-query": "^5.84.2",
    "@vercel/analytics": "^1.5.0",
    "date-fns": "^2.30.0",
    "minimatch": "^10.0.3",
    "next": "15.4.3",
    "next-pwa": "^5.6.0",
    "openai": "^5.12.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sanitize-html": "^2.17.0",
    "sonner": "^2.0.7",
    "web-push": "^3.6.7",
    "workbox-webpack-plugin": "^7.3.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.4.3",
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/next-pwa": "^5.6.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/sanitize-html": "^2.16.0",
    "@types/web-push": "^3.6.4",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "cli-real-favicon": "^0.0.9",
    "eslint": "^9",
    "eslint-config-next": "15.4.3",
    "ignore-loader": "^0.1.2",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "sharp": "^0.34.3",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.19.2",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ie_mob 11"
  ]
}
